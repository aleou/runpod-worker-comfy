{
  "input": {
    "workflow": {
      "1": {
        "inputs": {
          "video": "input_video.mp4",
          "force_rate": 0,
          "custom_width": 0,
          "custom_height": 0,
          "frame_load_cap": 0,
          "skip_first_frames": 0,
          "select_every_nth": 1,
          "format": "AnimateDiff"
        },
        "class_type": "VHS_LoadVideo",
        "_meta": {
          "title": "Load Video (Upload)"
        }
      },
      "2": {
        "inputs": {
          "ckpt_name": "realisticVisionV51_v51VAE.safetensors"
        },
        "class_type": "CheckpointLoaderSimple",
        "_meta": {
          "title": "Load Checkpoint"
        }
      },
      "3": {
        "inputs": {
          "text": "watermark, logo, branding",
          "clip": ["2", 1]
        },
        "class_type": "CLIPTextEncode",
        "_meta": {
          "title": "CLIP Text Encode (Positive)"
        }
      },
      "4": {
        "inputs": {
          "text": "blurry, low quality",
          "clip": ["2", 1]
        },
        "class_type": "CLIPTextEncode",
        "_meta": {
          "title": "CLIP Text Encode (Negative)"
        }
      },
      "5": {
        "inputs": {
          "images": ["1", 0],
          "vae": ["2", 2]
        },
        "class_type": "VAEEncode",
        "_meta": {
          "title": "VAE Encode"
        }
      },
      "6": {
        "inputs": {
          "seed": 42,
          "steps": 2,
          "cfg": 1.0,
          "sampler_name": "euler",
          "scheduler": "normal",
          "denoise": 0.5,
          "model": ["2", 0],
          "positive": ["3", 0],
          "negative": ["4", 0],
          "latent_image": ["5", 0]
        },
        "class_type": "KSampler",
        "_meta": {
          "title": "KSampler"
        }
      },
      "7": {
        "inputs": {
          "samples": ["6", 0],
          "vae": ["2", 2]
        },
        "class_type": "VAEDecode",
        "_meta": {
          "title": "VAE Decode"
        }
      },
      "8": {
        "inputs": {
          "frame_rate": 30,
          "loop_count": 0,
          "filename_prefix": "watermarked_video",
          "format": "video/h264-mp4",
          "pix_fmt": "yuv420p",
          "crf": 19,
          "save_metadata": true,
          "images": ["7", 0]
        },
        "class_type": "VHS_VideoCombine",
        "_meta": {
          "title": "Video Combine"
        }
      }
    },
    "files": [
      {
        "name": "input_video.mp4",
        "url": "https://your-bucket.s3.amazonaws.com/videos/your-video.mp4"
      }
    ]
  }
}
